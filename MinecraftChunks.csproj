        <PackageReference Include="Chickensoft.SuperNodes.Types" Version="1.8.0" />
        <PackageReference Include="Chickensoft.GameTools" Version="2.0.21" />
        <PackageReference Include="BG4G.Core" Version="1.0.3" />
    </ItemGroup>
</Project>